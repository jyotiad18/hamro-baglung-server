{
    "name": "hamro-baglung-server",
    "version": "1.0.0",
    "description": "hamro-baglung-server application with react, express, api  with mysql",
    "scripts": {
        "start": "npm-run-all --parallel watch:server start:web",
        "start:server": "node ./bin/www",
        "start:web": "react-scripts start",
        "watch:server": "nodemon --watch ./bin/www",
        "build": "react-scripts build",
        "test": "react-script test --env=jsdom",
        "eject": "react-script eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix --ext .js, ."
    },
    "keywords": [
        "nodejs",
        "express",
        "mysql",
        "restapi",
        "sequalize"
    ],
    "author": "jyotiadhikari",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.3",
        "helmet": "^3.23.1",
        "joi": "^14.3.1",
        "jsonwebtoken": "^8.5.1",
        "mysql": "^2.18.1",
        "mysql2": "^2.1.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^3.4.1",
        "sequelize": "^5.22.2",
        "sequelize-cli": "^6.1.0"
    },
    "devDependencies": {
        "eslint": "^7.3.1",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
